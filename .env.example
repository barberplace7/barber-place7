# Connect to Supabase via connection pooling
DATABASE_URL="postgresql://postgres.YOUR_PASSWORD@YOUR_PROJECT.pooler.supabase.com:6543/postgres?pgbouncer=true"

# Direct connection to the database. Used for migrations
DIRECT_URL="postgresql://postgres.YOUR_PASSWORD@YOUR_PROJECT.pooler.supabase.com:5432/postgres"

# Supabase Client Keys
NEXT_PUBLIC_SUPABASE_URL="https://YOUR_PROJECT.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your_anon_key_here"
SUPABASE_SERVICE_ROLE_KEY="your_service_role_key_here"

# JWT Secret for authentication
JWT_SECRET="your-secret-key-here"